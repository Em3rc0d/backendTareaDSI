{
    "version": 2,
    "builds": [
        {
            "src": "api/server.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/server.js"
        }
    ],
    "functions": {
        "api/server.js": {
            "memory": 1024
        }
    }
}